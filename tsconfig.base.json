        "packages/wallet-connect/src/index.ts"
      ],
      "@peersyst/ws-wallet-utils": [
        "packages/wallet-utils/src/index.ts"
      ],
      "@peersyst/ws-welldone-wallet": [
        "packages/welldone-wallet/src/index.ts"
      ],
      "@peersyst/ws-ramper-wallet": [
        "packages/ramper-wallet/src/index.ts"
      ],
      "@peersyst/ws-xdefi": [
        "packages/xdefi/src/index.ts"
      ],
      "@peersyst/ws-near-mobile-wallet": [
        "packages/near-mobile-wallet/src/index.ts"
      ],
      "@peersyst/ws-mintbase-wallet": [
        "packages/mintbase-wallet/src/index.ts"
      ],
      "@peersyst/ws-bitte-wallet": [
        "packages/bitte-wallet/src/index.ts"
