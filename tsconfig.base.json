{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2017",
      "dom"
    ],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "strictPropertyInitialization": false,
    "paths": {
      "@near-js/types": [
        "node_modules/@near-js/types"
      ],
      "@near-js/keystores": [
        "node_modules/@near-js/keystores"
      ],
      "@peersyst/ws-account-export": [
        "packages/account-export/src/index.ts"
      ],
      "@peersyst/ws-arepa-wallet": [
        "packages/arepa-wallet/src/index.ts"
      ],
      "@peersyst/ws-bitget-wallet": [
        "packages/bitget-wallet/src/index.ts"
      ],
      "@peersyst/ws-coin98-wallet": [
        "packages/coin98-wallet/src/index.ts"
      ],
      "@peersyst/ws-core": [
        "packages/core/src/index.ts"
      ],
      "@peersyst/ws-here-wallet": [
        "packages/here-wallet/src/index.ts"
      ],
      "@peersyst/ws-ledger": [
        "packages/ledger/src/index.ts"
      ],
      "@peersyst/ws-math-wallet": [
        "packages/math-wallet/src/index.ts"
      ],
      "@peersyst/ws-meteor-wallet": [
        "packages/meteor-wallet/src/index.ts"
      ],
      "@peersyst/ws-modal-ui": [
        "dist/packages/modal-ui"
      ],
      "@peersyst/ws-modal-ui-js": [
        "packages/modal-ui-js/src/index.ts"
      ],
      "@peersyst/ws-my-near-wallet": [
        "packages/my-near-wallet/src/index.ts"
      ],
      "@peersyst/ws-narwallets": [
        "packages/narwallets/src/index.ts"
      ],
      "@peersyst/ws-near-snap": [
        "packages/near-snap/src/index.ts"
      ],
      "@peersyst/ws-nearfi": [
        "packages/nearfi/src/index.ts"
      ],
      "@peersyst/ws-neth": [
        "packages/neth/src/index.ts"
      ],
      "@peersyst/ws-nightly": [
        "packages/nightly/src/index.ts"
      ],
      "@peersyst/ws-sender": [
        "packages/sender/src/index.ts"
      ],
