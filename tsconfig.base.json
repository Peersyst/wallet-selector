      ],
      "https": [
        "node_modules/https-browserify"
      ],
      "stream": [
        "node_modules/stream-browserify"
      ],
      "url": [
        "node_modules/url"
      ],
      "fs": [
        "node_modules/browserify-fs"
      ],
      "path": [
        "node_modules/path-browserify"
      ],
      "react": [
        "node_modules/@types/react"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
